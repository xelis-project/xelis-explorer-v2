{
    "name": "xelis-explorer-v2",
    "version": "0.1.757",
    "type": "module",
    "scripts": {
        "dev": "vite dev --host",
        "dev-testnet": "npm run dev -- --mode testnet",
        "dev-mainnet": "npm run dev -- --mode mainnet",
        "build": "vite build --mode mainnet",
        "preview": "npm run build && vite preview",
        "deploy": "npm run build && wrangler deploy",
        "build-testnet": "vite build --mode testnet",
        "deploy-testnet": "cross-env CLOUDFLARE_ENV=testnet npm run build-testnet && wrangler deploy",
        "cf-typegen": "wrangler types --env-interface CloudflareBindings",
        "find-txs": "npx tsx ./scripts/find_txs.ts",
        "gen-lang": "npx tsx ./scripts/lang_dict.ts --src ./src --out ./lang.json"
    },
    "dependencies": {
        "@alenaksu/json-viewer": "^2.1.2",
        "@emeraldpay/hashicon": "^0.5.2",
        "@xelis/sdk": "^0.11.35",
        "animejs": "^4.2.2",
        "camera-controls": "^3.1.0",
        "d3": "^7.9.0",
        "flag-icons": "^7.5.0",
        "hono": "^4.9.2",
        "human-number": "^2.0.7",
        "js-cookie": "^3.0.5",
        "leaflet": "^1.9.4",
        "nouislider": "^15.8.1",
        "pretty": "^2.0.0",
        "pretty-bytes": "^7.1.0",
        "pretty-ms": "^9.3.0",
        "qrcode": "^1.5.4",
        "reset-css": "^5.0.2",
        "three": "^0.179.1",
        "troika-three-text": "^0.52.4",
        "urlpattern-polyfill": "^10.1.0"
    },
    "devDependencies": {
        "@cloudflare/vite-plugin": "^1.13.18",
        "@types/d3": "^7.4.3",
        "@types/human-number": "^1.0.2",
        "@types/js-cookie": "^3.0.6",
        "@types/leaflet": "^1.9.21",
        "@types/node-dir": "^0.0.37",
        "@types/pretty": "^2.0.3",
        "@types/qrcode": "^1.5.6",
        "@types/three": "^0.179.0",
        "@types/ws": "^8.18.1",
        "@types/yargs": "^17.0.34",
        "cross-env": "^10.1.0",
        "node-dir": "^0.1.17",
        "typescript": "^5.9.3",
        "vite": "^7.1.12",
        "vite-plugin-checker": "^0.10.3",
        "wrangler": "^4.45.3",
        "yargs": "^18.0.0"
    }
}
